{% extends 'base.html.twig' %}

{% block title %}Ajouter un stagiaire{% endblock %}

{% block body %}



    {# {{ form(formAddEmploye) }} #}

    {% if edit %}
    <h1>Editer un stagiaire</h1>
    {% else %}
    <h1>Ajouter un stagiaire</h1>
    {% endif %}


    {{ form_start(formAddStudent) }}

        
        <div>
            <div {{ form_row(formAddStudent.lastName) }}</div>
            <div {{ form_row(formAddStudent.firstName) }}</div>
        </div>

        <div>
            <div {{ form_row(formAddStudent.sexe) }}</div>
            <div {{ form_row(formAddStudent.birthday) }}</div>
        </div>

        <div>
            <div {{ form_row(formAddStudent.city) }}</div>
        </div>

        <div>
            <div {{ form_row(formAddStudent.email) }}</div>
            <div {{ form_row(formAddStudent.phoneNumber) }}</div>
        </div>
        
        
        
        {{ form_row(formAddStudent.Validate) }}
        

    {{ form_end(formAddStudent) }}


{% endblock %}